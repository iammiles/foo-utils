{
    "compilerOptions": {
      "target": "ES6",
      "module": "ES6",
      "outDir": "./dist",
      "strict": true,
      "lib": ["DOM","ES2019","ES2017", "ESNext"],
      "allowJs": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "rootDir": "src",
      "incremental": false,
      "skipLibCheck": true,
      "sourceMap": true
    },
    "include": [
        "src"
    ]
  }